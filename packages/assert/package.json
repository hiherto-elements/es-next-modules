{
  "name": "@hiherto-elements/assert",
  "version": "0.0.9",
  "description": "A simple assertion libbrary refactred to es6",
  "main": "Assert.js",
  "directories": {
    "test": "test"
  },
  "keywords": ["test", "tdd", "assert"],
  "private": false,
  "scripts": {
    "docs": "rm -rf ./docs && esdoc",
    "test": "sh test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiherto-elements/es-next-modules.git"
  },
  "esdoc": {
    "source": ".",
    "includes": [
      "Assert.js",
      "MessageFactory.js",
      "InvalidValueException.js",
      "ValueConverter.js",
      "test/test.js"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiherto-elements/es-next-modules/issues"
  },
  "homepage": "https://hiherto-elements.github.io/es-next-modules/assert/"
}
