{
  "name": "@hiherto-elements/diff",
  "version": "0.0.5",
  "description": "es6 version of jsDiff",
  "main": "Url.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "es6",
    "diff",
    "textprocessing"
  ],
  "esdoc": {
    "source": ".",
    "includes": [
      "diff.js",
      "array.js",
      "character.js",
      "css.js",
      "json.js",
      "line.js"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "devDependencies": {
    "@hiherto-elements/assert": "0.0.11"
  },
  "scripts": {
    "reset": "rm -rf node_modules && rm -rf package-lock.json",
    "docs": "rm -rf ./docs && esdoc",
    "test": "sh test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiherto-elements/es-next-modules.git"
  },
