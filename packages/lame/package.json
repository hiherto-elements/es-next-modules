{
  "name": "@hiherto-elements/lame",
  "version": "0.0.1",
  "description": "A mp3 encoder, forked from lame.js and ported to pure es6",
  "main": "lame.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "mp3",
    "encode",
    "music"
  ],
  "private": false,
  "scripts": {
    "reset": "rm -rf node_modules && rm -rf package-lock.json",
    "docs": "rm -rf ./docs && esdoc",
    "test": "sh test.sh"
  },
  "dependencies": {
    "@hiherto-elements/assert": "0.0.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiherto-elements/es-next-modules.git"
  },
  "esdoc": {
    "source": ".",
    "includes": [],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiherto-elements/es-next-modules/issues"
  },
  "homepage": "https://hiherto-elements.github.io/es-next-modules/lame/"
}
