{
  "name": "@hiherto-elements/querystring",
  "version": "0.0.2",
  "description": "es6 version of the original node js url module",
  "main": "querystring.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "es6",
    "url",
    "querystring",
    "parser"
  ],
  "esdoc": {
    "source": ".",
    "includes": [
      "encode.js",
      "decode.js"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "devDependencies": {
    "@hiherto-elements/assert": "0.0.12"
  },
  "scripts": {
    "reset": "rm -rf node_modules && rm -rf package-lock.json",
    "docs": "rm -rf ./docs && esdoc",
    "test": "sh test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiherto-elements/es-next-modules.git"
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiherto-elements/es-next-modules/issues"
  },
  "homepage": "https://hiherto-elements.github.io/es-next-modules/querystring/"
}
