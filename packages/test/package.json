{
  "name": "@hiherto-elements/test",
  "version": "0.0.2",
  "description": "a very simple test runner, delivered as es module",
  "main": "test.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "es6",
    "tdd",
    "testrunner",
    "bdd"
  ],
  "esdoc": {
    "source": ".",
    "includes": [
      "test.js",
      "tdd.js"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "devDependencies": {
    "@hiherto-elements/assert": "0.0.11"
  },
  "scripts": {
    "reset": "rm -rf node_modules && rm -rf package-lock.json",
    "docs": "rm -rf ./docs && esdoc",
    "test": "sh test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiherto-elements/es-next-modules.git"
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiherto-elements/es-next-modules/issues"
  },
  "homepage": "https://hiherto-elements.github.io/es-next-modules/test/"
}
