{
  "name": "@hiherto-elements/gherkin",
  "version": "0.0.6",
  "description": "minimal es6 test gherkin parser",
  "main": "parse.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@hiherto-elements/assert": "0.0.11"
  },
  "scripts": {
    "reset": "rm -rf node_modules && rm -rf package-lock.json",
    "test": "sh test.sh",
    "docs": "rm -rf ./docs && esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiherto-elements/es-next-modules.git"
  },
  "keywords": [
    "es6",
    "bdd",
    "gherkin",
    "cucumber",
    "parser"
  ],
  "esdoc": {
    "source": ".",
    "includes": [
      "parse.js",
      "gherkin.js"
    ],
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiherto-elements/es-next-modules/issues"
  },
  "homepage": "https://hiherto-elements.github.io/es-next-modules/gherkin/"
}
